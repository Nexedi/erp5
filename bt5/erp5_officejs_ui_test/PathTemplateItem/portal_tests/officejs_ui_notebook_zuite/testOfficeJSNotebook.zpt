        </tr>
        <tr>
          <td>type</td>
          <td>//input[@title='Language']</td>
          <td>${language}</td>
        </tr>
        <tr>
          <td>type</td>
          <td>//textarea[@title='Description']</td>
          <td>${description}</td>
        </tr>
        <tr>
          <td>click</td>
          <td>//button[@data-i18n='Save']</td>
          <td></td>
