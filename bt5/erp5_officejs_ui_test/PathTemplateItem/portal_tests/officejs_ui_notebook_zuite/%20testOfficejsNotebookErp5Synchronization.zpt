        <tr>
          <td>store</td>
          <td tal:content="python: 'Test Description %s' % DateTime()">Description</td>
          <td>description</td>
        </tr>
        <tr>
          <td>waitForElementPresent</td>
          <td>//input[@title='Title']</td>
          <td></td>
        </tr>
