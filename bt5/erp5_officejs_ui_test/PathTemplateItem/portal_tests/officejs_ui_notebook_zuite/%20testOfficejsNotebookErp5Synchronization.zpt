        <tr>
          <td>click</td>
          <td>//a[@data-i18n="Storages"]</td>
          <td></td>
        </tr>
        <tr>
          <td>waitForElementPresent</td>
          <td>link=Erp5</td>
          <td></td>
        </tr>
        <tr>
          <td>click</td>
          <td>link=Erp5</td>
          <td></td>
        </tr>
        <tr>
          <td>waitForElementPresent</td>
          <td>name=erp5_url</td>
          <td></td>
        </tr>
