          <td>waitForElementPresent</td>
          <td>id=title</td>
          <td></td>
        </tr>
        <tr>
          <td>store</td>
          <td tal:content="python: 'test_text_%s' % DateTime().strftime('%Y%m%d%H%M%S')">Titre</td>
          <td>title</td>
        </tr>
        <tr>
          <td>store</td>
          <td tal:content="python: 'Test Reference %s' % DateTime()">Reference</td>
          <td>reference</td>
        </tr>
        <tr>
          <td>store</td>
