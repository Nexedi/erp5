            <key> <string>text_content</string> </key>
            <value> <string encoding="cdata"><![CDATA[

/*jslint indent: 2, maxerr: 3, nomen: true */\n
/*global window, rJS, RSVP, $ */\n
(function (window, rJS, RSVP, $) {\n
  "use strict";\n
  rJS(window)\n
    /////////////////////////////////////////////////////////////////\n
    // ready\n
    /////////////////////////////////////////////////////////////////\n
    // Init local properties\n
    .ready(function (g) {\n
      g.props = {};\n
    })\n
    .ready(function (g) {\n
      return g.getElement()\n
        .push(function (element) {\n
          g.props.element = element;\n
          g.props.container = element.querySelector(".jqm-navmenu-panel");\n
          g.props.jelement = $(g.props.container);\n
        });\n
    })\n
    .ready(function (g) {\n
      g.props.jelement.panel({\n
        display: "overlay",\n
        position: "right",\n
        theme: "c"\n
      });\n
    })\n
\n
\n
    .allowPublicAcquisition(\'trigger\', function () {\n
      return this.props.jelement.panel("toggle");\n
    })\n
    //////////////////////////////////////////////\n
    // acquired method\n
    //////////////////////////////////////////////\n
    //local method\n
    .declareMethod(\'close\', function () {\n
      var container = this.props.container;\n
      this.props.jelement.panel("close");\n
      while (container.firstChild) {\n
        container.removeChild(container.firstChild);\n
      }\n
    })\n
    .declareMethod(\'render\', function (url, options) {\n
      var gadget = this,\n
        declared_gadget;\n
      if (url && gadget.props.options !== options) {\n
        gadget.props.options = options;\n
        return new RSVP.Queue()\n
          .push(function () {\n
            return gadget.declareGadget(url, {scope: "declared_gadget"});\n
          })\n
          .push(function (result) {\n
            declared_gadget = result;\n
            return declared_gadget.render(options);\n
          })\n
          .push(function () {\n
            return RSVP.all([\n
              gadget.close(),\n
              declared_gadget.getElement()\n
            ]);\n
          })\n
          .push(function (result) {\n
            var fragment = result[1];\n
            gadget.props.container.appendChild(fragment);\n
            gadget.props.jelement.trigger("create");\n
            gadget.props.jelement.panel("toggle");\n
          });\n
      }\n
      gadget.props.jelement.panel("toggle");\n
    });\n
}(window, rJS, RSVP, $));

]]></string> </value>
