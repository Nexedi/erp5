<tr><td>click</td>
    <td>//a[@data-i18n="Export"]</td><td></td></tr>

<tr><td>waitForElementPresent</td>
    <td>//a[@data-i18n="General Ledger"]</td><td></td></tr>
<tr><td>click</td>
