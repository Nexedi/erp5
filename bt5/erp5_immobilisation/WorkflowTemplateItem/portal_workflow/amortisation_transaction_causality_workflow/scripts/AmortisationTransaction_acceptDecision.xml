        <item>
            <key> <string>_body</string> </key>
            <value> <string>transaction = state_change[\'object\']\n
context.log(\'acceptDecision script on\', transaction)\n
transaction.portal_simulation.solveDelivery(transaction, "Distribute",\n
                                             "ProfitAndLoss")\n
for movement in transaction.getMovementList():\n
  for simulation_movement in movement.getDeliveryRelatedValueList(portal_type=\'Simulation Movement\'):\n
    context.log(\'solving movement\', simulation_movement)\n
    transaction.portal_simulation.solveMovement(simulation_movement, None,\n
                                             "ProfitAndLoss")\n
\n
# Automatic workflow\n
transaction.activate().updateCausalityState()\n
