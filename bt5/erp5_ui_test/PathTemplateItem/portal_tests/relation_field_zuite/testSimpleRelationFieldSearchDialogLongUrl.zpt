  <td></td>
</tr>
<tr>
  <td>open</td>
  <td>${base_url}/foo_module/view</td>
  <td></td>
</tr>
<tr>
  <td>clickAndWait</td>
  <td>link=0</td>
  <td></td>
</tr>
<tr>
  <td>type</td>
  <td>field_my_title</td>
  <td></td>
</tr>
<tr>
  <td>type</td>
  <td>field_my_short_title</td>
  <td></td>
</tr>
<tr>
  <td>type</td>
  <td>field_my_reference</td>
  <td></td>
</tr>
<tr>
  <td>clickAndWait</td>
  <td>//input[preceding-sibling::input[@name="field_my_foo_category_title"]][@value="update..."]</td>
  <td></td>
</tr>

<tr>
  <td>type</td>
  <td>//input[@name="listbox_title"]</td>
  <td>%1%</td>
</tr>
<tr>
  <td>keyPressAndWait</td>
  <td>//input[@name="listbox_title"]</td>
  <td>\13</td> <!-- ENTER -->
</tr>

<tr>
  <td>type</td>
  <td>//input[@name="listbox_title"]</td>
  <td>%2%</td>
</tr>
<tr>
  <td>keyPressAndWait</td>
  <td>//input[@name="listbox_title"]</td>
  <td>\13</td> <!-- ENTER -->
</tr>

<tr>
  <td>type</td>
  <td>//input[@name="listbox_title"]</td>
  <td>%2%</td>
</tr>
<tr>
  <td>keyPressAndWait</td>
  <td>//input[@name="listbox_title"]</td>
  <td>\13</td> <!-- ENTER -->
</tr>

<tr>
  <td>click</td>
  <td>//input[@type="checkbox"]</td>
  <td></td>
</tr>
<tr>
  <td>clickAndWait</td>
  <td>id=dialog_submit_button</td>
  <td></td>
</tr>
<tr>
  <td>verifyValue</td>
  <td>field_my_foo_category_title</td>
  <td>a2</td>
</tr>

  </tbody></table>
