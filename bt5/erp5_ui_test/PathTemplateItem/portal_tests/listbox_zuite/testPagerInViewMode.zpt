<html xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal">
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Test Pager in View Mode</title>
</head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="3">Test Pager in View Mode</td></tr>
</thead><tbody>
<tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/init" />
<tr>
  <td>open</td>
  <td>&#36;{base_url}/foo_module/FooModule_createObjects?num:int=1</td>
  <td></td>
</tr>
<tr>
  <td>assertTextPresent</td>
  <td>Created Successfully.</td>
  <td></td>
</tr>
<tr>
  <td>open</td>
  <td>&#36;{base_url}/foo_module/Zuite_waitForActivities</td>
  <td></td>
</tr>
<tr>
  <td>assertTextPresent</td>
  <td>Done.</td>
  <td></td>
</tr>
<tr>
  <td>open</td>
  <td>&#36;{base_url}/ListBoxZuite_resetReportSelections</td>
  <td></td>
</tr>
<tr>
  <td>assertTextPresent</td>
  <td>Done.</td>
  <td></td>
</tr>
<tr>
  <td>open</td>
  <td>&#36;{base_url}/foo_module/0/Foo_viewListBoxInReportModeReport</td>
  <td></td>
</tr>

<tal:block tal:define="prefix string:x0_listbox; 
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>


<tal:block tal:repeat="prefix python:range(3)">
<tr>
  <td>verifyElementPresent</td>
  <td tal:content="string: //button[@class=&quot;listbox_next_page x${prefix}_listbox_next_page&quot;]"></td>
  <td></td>
</tr>
<tr>
  <td>verifyElementNotPresent</td>
  <td tal:content="string: //button[@class=&quot;listbox_previous_page x${prefix}_listbox_previous_page&quot;]"></td>
  <td></td>
</tr>
</tal:block>
<tr>
  <td>clickAndWait</td>
  <td>//button[@class="listbox_next_page x0_listbox_next_page"]</td>
  <td></td>
</tr>
<tal:block tal:define="prefix string:x0_listbox; 
                       set_page python:2;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x1_listbox; 
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x2_listbox; 
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:condition="python: context.TestTool_getSkinName()!='Mobile'">
<tr>
  <td>type</td>
  <td>//input[@class="listbox_set_page x1_listbox_set_page"]</td>
  <td>5</td>
</tr>
<tr>
  <td>keyPressAndWait</td>
  <td>//input[@class="listbox_set_page x1_listbox_set_page"]</td>
  <td>\13</td>
</tr>
</tal:block>
<tal:block tal:condition="python: context.TestTool_getSkinName()=='Mobile'">
<tr>
  <td>select</td>
  <td>id=listbox_page_selection</td>
  <td>5 of 10</td>
</tr>
</tal:block>

<tal:block tal:define="prefix string:x0_listbox; 
                       set_page python:2;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x1_listbox; 
                       set_page python:5;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x2_listbox; 
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>
<tr>
  <td>clickAndWait</td>
  <td>//button[@class="listbox_previous_page x1_listbox_previous_page"]</td>
  <td></td>
</tr>

<tal:block tal:define="prefix string:x0_listbox; 
                       set_page python:2;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x1_listbox; 
                       set_page python:4;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tal:block tal:define="prefix string:x2_listbox; 
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>
<tr>
  <td>clickAndWait</td>
  <td>//button[@class="listbox_last_page x1_listbox_last_page"]</td>
  <td></td>
</tr>

<tal:block tal:define="prefix string:x0_listbox; 
                       set_page python:2;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>

<tr>
  <td>clickAndWait</td>
  <td>//button[@class="listbox_first_page x1_listbox_first_page"]</td>
  <td></td>
</tr>
<tal:block tal:define="prefix string:x0_listbox;
                       set_page python:2;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>
<tal:block tal:define="prefix string:x1_listbox;
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>
<tal:block tal:define="prefix string:x2_listbox;
                       set_page python:1;
                       lines_per_pages python:3">
  <tal:block metal:use-macro="here/ListBoxZuite_CommonTemplate/macros/checkCurrentPage" />
</tal:block>
</tbody></table>
</body>
</html>
