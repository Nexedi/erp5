        <item>
            <key> <string>src</string> </key>
            <value> <string encoding="cdata"><![CDATA[

SELECT\n
   YEAR(movement.target_stop_date) as target_stop_date,\n
   movement.variation_text AS variation_text,\n
   destination.title as destination_title,\n
  - SUM(movement.total_price) as total_price\n
FROM\n
  catalog\n
LEFT JOIN\n
  movement ON (movement.uid = catalog.uid)\n
LEFT JOIN \n
  catalog AS destination ON (movement.destination_uid = destination.uid)\n
WHERE\n
  catalog.portal_type = "Pay Sheet Cell" <dtml-if year>and YEAR(movement.target_stop_date)=<dtml-sqlvar year type="int"> </dtml-if> \n
GROUP BY\n
  movement.target_stop_date, movement.destination_uid, movement.variation_text

]]></string> </value>
