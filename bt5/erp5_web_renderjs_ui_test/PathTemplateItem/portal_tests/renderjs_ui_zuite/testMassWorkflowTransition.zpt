  <tal:block metal:use-macro="here/Zuite_CommonTemplateForRenderjsUi/macros/wait_for_notification" />
</tal:block>

<tr><td>waitForElementPresent</td>
    <td>//textarea[@name="field_workflow_form_your_comment"]</td><td></td></tr>
<tr><td>type</td>
