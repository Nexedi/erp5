<tr><td>assertText</td>
    <td>//div[contains(@data-gadget-url, 'gadget_erp5_field_listbox.html')]//table/tbody/tr[2]/td[3]//a</td>
    <td>validated</td></tr>
<tr><td>assertText</td>
    <td>//div[contains(@data-gadget-url, 'gadget_erp5_field_listbox.html')]//table/tbody/tr[3]/td[3]//a</td>
