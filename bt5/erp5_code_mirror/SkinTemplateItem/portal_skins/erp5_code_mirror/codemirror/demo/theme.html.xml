<article>\n
<h2>Theme Demo</h2>\n
<form><textarea id="code" name="code">\n
function findSequence(goal) {\n
  function find(start, history) {\n
    if (start == goal)\n
      return history;\n
    else if (start > goal)\n
      return null;\n
    else\n
      return find(start + 5, "(" + history + " + 5)") ||\n
             find(start * 3, "(" + history + " * 3)");\n
  }\n
  return find(1, "1");\n
}</textarea></form>\n
