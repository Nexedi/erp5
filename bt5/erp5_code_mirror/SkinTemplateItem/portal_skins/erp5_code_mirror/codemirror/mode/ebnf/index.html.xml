    <article>\n
      <h2>EBNF Mode (bracesMode setting = "javascript")</h2>\n
      <form><textarea id="code" name="code">\n
/* description: Parses end executes mathematical expressions. */\n
\n
/* lexical grammar */\n
%lex\n
\n
%%\n
\\s+                   /* skip whitespace */\n
[0-9]+("."[0-9]+)?\\b  return \'NUMBER\';\n
"*"                   return \'*\';\n
"/"                   return \'/\';\n
"-"                   return \'-\';\n
"+"                   return \'+\';\n
"^"                   return \'^\';\n
"("                   return \'(\';\n
")"                   return \')\';\n
"PI"                  return \'PI\';\n
"E"                   return \'E\';\n
&lt;&lt;EOF&gt;&gt;               return \'EOF\';\n
\n
/lex\n
\n
/* operator associations and precedence */\n
\n
%left \'+\' \'-\'\n
%left \'*\' \'/\'\n
%left \'^\'\n
%left UMINUS\n
\n
%start expressions\n
\n
%% /* language grammar */\n
\n
expressions\n
: e EOF\n
{print($1); return $1;}\n
;\n
\n
e\n
: e \'+\' e\n
{$$ = $1+$3;}\n
| e \'-\' e\n
{$$ = $1-$3;}\n
| e \'*\' e\n
{$$ = $1*$3;}\n
| e \'/\' e\n
{$$ = $1/$3;}\n
| e \'^\' e\n
{$$ = Math.pow($1, $3);}\n
| \'-\' e %prec UMINUS\n
{$$ = -$2;}\n
| \'(\' e \')\'\n
{$$ = $2;}\n
| NUMBER\n
{$$ = Number(yytext);}\n
| E\n
{$$ = Math.E;}\n
| PI\n
{$$ = Math.PI;}\n
;</textarea></form>\n
