<article>\n
<h2>Elm mode</h2>\n
\n
<div><textarea id="code" name="code">\n
import Color exposing (..)\n
import Graphics.Collage exposing (..)\n
import Graphics.Element exposing (..)\n
import Time exposing (..)\n
\n
main =\n
  Signal.map clock (every second)\n
\n
clock t =\n
  collage 400 400\n
    [ filled    lightGrey   (ngon 12 110)\n
    , outlined (solid grey) (ngon 12 110)\n
    , hand orange   100  t\n
    , hand charcoal 100 (t/60)\n
    , hand charcoal 60  (t/720)\n
    ]\n
\n
hand clr len time =\n
  let angle = degrees (90 - 6 * inSeconds time)\n
  in\n
      segment (0,0) (fromPolar (len,angle))\n
        |> traced (solid clr)\n
</textarea></div>\n
\n
