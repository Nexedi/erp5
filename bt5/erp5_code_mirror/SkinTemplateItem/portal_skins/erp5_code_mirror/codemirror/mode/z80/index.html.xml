<h2>Z80 assembly mode</h2>\n
\n
\n
<div><textarea id="code" name="code">\n
#include    "ti83plus.inc"\n
#define     progStart   $9D95\n
    .org progStart-2\n
    .db $BB,$6D\n
\n
    bcall(_ClrLCDFull)\n
    ld hl,0\n
    ld (CurCol),hl\n
    ld hl,Message\n
    bcall(_PutS) ; Displays the string\n
    bcall(_NewLine)\n
    ret\n
Message:\n
    .db "Hello world!",0\n
</textarea></div>\n
