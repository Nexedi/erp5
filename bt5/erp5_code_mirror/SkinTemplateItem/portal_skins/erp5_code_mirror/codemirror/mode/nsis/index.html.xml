<h2>NSIS mode</h2>\n
\n
\n
<textarea id=code>\n
; This is a comment\n
!ifdef ERROR\n
    !error "Something went wrong"\n
!endif\n
\n
OutFile "demo.exe"\n
RequestExecutionLevel user\n
SetDetailsPrint listonly\n
\n
!include "LogicLib.nsh"\n
!include "WinVer.nsh"\n
\n
Section -mandatory\n
\n
    Call logWinVer\n
\n
    ${If} 1 > 0\n
      MessageBox MB_OK "Hello world"\n
    ${EndIf}\n
\n
SectionEnd\n
\n
Function logWinVer\n
\n
    ${If} ${IsWin10}\n
        DetailPrint "Windows 10!"\n
    ${ElseIf} ${AtLeastWinVista}\n
        DetailPrint "We\'re post-XP"\n
    ${Else}\n
        DetailPrint "Legacy system"\n
    ${EndIf}\n
\n
FunctionEnd\n
</textarea>\n
