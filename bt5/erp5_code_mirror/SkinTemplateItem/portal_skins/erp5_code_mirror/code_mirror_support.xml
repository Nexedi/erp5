 var mode = "&dtml-mode;";\n
 if ( mode === "sql" ) {\n
   mode = "text/x-mariadb";\n
 }\n
\n
<dtml-if expr="mode == \'python\'">\n
 lint_option = {"getAnnotations": checkPythonSourceCode,\n
