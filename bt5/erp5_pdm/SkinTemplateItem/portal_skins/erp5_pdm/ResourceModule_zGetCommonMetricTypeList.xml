      GROUP BY metric_type_uid\n
     ) AS measure,\n
     catalog\n
WHERE catalog.uid = measure.metric_type_uid\n
  AND <dtml-sqltest column="measure.resource_count"\n
                    expr="_.len(resource_uid)"\n
                    type="int">\n


]]></string> </value>
