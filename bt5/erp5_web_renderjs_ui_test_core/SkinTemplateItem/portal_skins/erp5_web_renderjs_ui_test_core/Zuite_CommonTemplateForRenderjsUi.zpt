      <td>//div[@data-gadget-scope='header']//h1//*[contains(@class, "ui-icon-spinner")]</td>
      <td></td>
    </tr>
    <!-- XXX Sadly, spinner currently is dropped before all DOM modifications are done.
         Until this is fixed, we use the ugly pause command -->
    <tr>
      <td>pause</td>
      <td>100</td>
      <td></td>
    </tr>
    <tr>
      <td colspan="3"><p></p></td>
    </tr>
