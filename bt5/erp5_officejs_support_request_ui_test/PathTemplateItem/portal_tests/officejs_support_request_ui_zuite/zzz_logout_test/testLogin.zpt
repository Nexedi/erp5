</tr>
<tr>
  <td>type</td>
  <td>//input[@name='__ac_name']</td>
  <td>user_logout_test</td>
</tr>
<tr>
  <td>type</td>
  <td>//input[@name='__ac_password']</td>
  <td>user_logout_test</td>
</tr>
<tr>
	<td>click</td>
	<td>WebSite_login:method</td>
	<td></td>
</tr>
<tr>
  <td>waitForElementPresent</td>
  <td>//div[contains(@data-gadget-url, 'gadget_erp5_header.html')]//h1</td>
