        <item>
            <key> <string>_body</string> </key>
            <value> <string>bank_account = state_change.object\n
\n
bank_account_id = bank_account.getId()\n
clean_bank_account_id = bank_account_id.replace(\' \', \'\')\n
\n
if bank_account_id != clean_bank_account_id:\n
  bank_account_id = clean_bank_account_id\n
  bank_account.setId(bank_account_id)\n
\n
bank_account.setBankCode(bank_account_id[0])\n
bank_account.setBranch(bank_account_id[1:3])\n
bank_account.setBankAccountNumber(bank_account_id)\n
</string> </value>
