        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.ERP5Type.Message import Message\n
portal = context.getPortalObject()\n
N_ = lambda msg, **kw: Message(\'erp5_ui\', msg, **kw)\n
precision = portal.ERP5Accounting_getParams().get(\'precision\', 2)\n
\n
# update selection\n
selection_name="accounting_upcoming_annotation_selection"\n
portal.portal_selections.updateSelectionCheckedUidList(selection_name,\n
                                                      listbox_uid, uids)\n
portal.portal_selections.setSelectionParamsFor(selection_name,\n
          params=dict(source=source, destination=destination,))\n
\n
return context.ERP5Site_redirect(\n
    \'%s/AccountingTransactionModule_viewUpcomingAnnotationDialog\' %\n
    portal.absolute_url(), keep_items=dict(\n
            precision=precision, amount=amount, side=side, at_date=at_date,\n
            from_date=from_date, cancel_url=cancel_url,\n
            portal_status_message=N_(\'Updated\')))\n
</string> </value>
