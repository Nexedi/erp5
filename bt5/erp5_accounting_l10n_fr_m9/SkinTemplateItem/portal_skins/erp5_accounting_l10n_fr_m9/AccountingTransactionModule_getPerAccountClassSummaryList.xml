        <item>
            <key> <string>_body</string> </key>
            <value> <string>from Products.PythonScripts.standard import Object\n
cache_storage = context.REQUEST.other\n
line_list = []\n
portal = context.getPortalObject()\n
total_debit = 0.00\n
total_credit = 0.00\n
\n
account_cache = cache_storage[\'m9_report_per_account_summary\'][\n
                            kw[\'selection\'].getParams()[\'classe\']]\n
account_list = account_cache.keys()\n
account_list.sort()\n
\n
for account_url in account_list:\n
  total_price = account_cache[account_url]\n
  debit = round(max(total_price, 0), 2)\n
  total_debit += debit\n
  credit = round(-min(total_price, 0), 2)\n
  total_credit += credit\n
  \n
  line_list.append(Object(uid=\'new_\',\n
                        account_id=account_url.split(\'/\')[-1],\n
                        debit=debit,\n
                        credit=credit))\n
\n
cache_storage[\'m9_report_per_account_summary_total_debit\'] = total_debit\n
cache_storage[\'m9_report_per_account_summary_total_credit\'] = total_credit\n
return line_list\n
</string> </value>
