\n
priority = 3\n
\n
activate_kw = { \n
  \'tag\': expand_tag,\n
  \'priority\': priority,\n
}\n
delivery.activate(\n
    after_path_and_method_id=path_and_method_id,\n
    tag=expand_tag,\n
    priority=priority,\n
    ).Delivery_updateAppliedRule(activate_kw=activate_kw)\n


