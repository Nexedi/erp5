          <ul data-role="listview" data-inset="true">\n
            {{#each document_list}}\n
              <li><a href="{{url}}">\n
                {{title}} || {{reference}} || {{description}}\n
                </a>\n
              </li>\n
            {{else}}\n
              <li><p>Empty</p></li>\n
            {{/each}}\n
