.tabbertab a.action_number{
  text-decoration: underline;
  font-weight: bold;
  font-size: 1.2em;
}

.tabbertab ul{
  list-style: none;
}

.search_tab img{
  margin-right: 25px;
}

#search {
  float:left;
  vertical-align:middle;
  margin:0px !important;
}

#search button {
background-color:inherit;
border:0pt none;
color:inherit;
cursor:pointer;
margin-left:0pt;
margin-right:2px;
margin-top:1px;
padding:0pt;
position:relative;
vertical-align:middle;
  display:inline;

/*  margin:0 !important;
  padding:0 !important;
  background-color:inherit;
  border:0pt none;
  color:inherit;
  cursor:pointer;
  display:inline;
  height: 28px;*/
}

#search button .image {
  background-repeat:no-repeat;
  display:block;
  height:22px;
  width:22px;
  background-image: url('img/search.png');
/*  width: 22px;
  height: 22px;
  padding-bottom: 9px;
  padding-left:22px;
  background-repeat: no-repeat;*/
}

#search button .description {
  display:none;
}


.application_folder{
  float: left;
  margin-right: 20px;
  background-image: url(img/folder2.png);
  height: 30px;
  padding-left: 30px;
