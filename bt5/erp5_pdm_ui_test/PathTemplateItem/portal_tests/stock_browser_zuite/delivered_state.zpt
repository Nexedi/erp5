<html>
<head><title>Test stock browser for delivered state</title></head>
<body>
<table cellpadding="1" cellspacing="1" border="1">
<thead>
<tr><td rowspan="1" colspan="4">
Test stock browser for delivered state
</td></tr>
</thead>
<tbody tal:define="init_method string:PdmZuite_createDelivery?state=delivered">

<!-- Setup {{{ -->
<tal:block metal:use-macro="here/PdmZuite_CommonTemplate/macros/init" />
<!-- }}} -->

<!-- Check source -->
<tal:block tal:define="link_title python: 'erp5_pdm_ui_test_source_site';
                       movement_count python: 1;
                       current_inventory python: -1.0;
                       available_inventory python: -1.0;
                       future_inventory python: -1.0;
                       ">
  <tal:block metal:use-macro="here/PdmZuite_checkStockBrowser/macros/init" />
</tal:block>

<!-- Check destination -->
<tal:block tal:define="link_title python: 'erp5_pdm_ui_test_destination_site';
                       movement_count python: 1;
                       current_inventory python: 1.0;
                       available_inventory python: 1.0;
                       future_inventory python: 1.0;
                       ">
  <tal:block metal:use-macro="here/PdmZuite_checkStockBrowser/macros/init" />
</tal:block>

</tbody></table>
</body>
</html> 
